{
	"name": "nature",
	"type": "dark",
	"colors": {
		"editor.background": "#242424",
		"editor.foreground": "#CCCCCC",
		"editor.lineHighlightBackground": "#1A1A1A",
		"editor.inactiveSelectionBackground": "#3A3D41",
		"editor.selectionHighlightBackground": "#ADD6FF26",
		"editorIndentGuide.background": "#BBBBBB",
		"editorIndentGuide.activeBackground": "#cfc141",
		"editorCursor.foreground": "#E6E6E6",
		"editorSuggestWidget.background": "#313131",
		"editorSuggestWidget.border": "#171717",
		"editorSuggestWidget.selectedBackground": "#171717",
		"editor.findMatchBackground": "#171717",
		"editor.findMatchBorder": "#FF9999",
		"editor.findMatchHighlightBackground": "#b0ffad26",
		"editorBracketMatch.background": "#171717",
		"editorBracketMatch.border": "#ffffff",
		"inputOption.activeBorder": "#ACD474",
		"inputValidation.warningBorder": "#E08B7E",
		"button.background": "#171717",
		"button.foreground": "#CCCCCC",
		"button.hoverBackground": "#242424",
		"list.dropBackground": "#171717",
		"list.hoverBackground": "#242424",
		"list.inactiveSelectionBackground": "#444444",
		"list.activeSelectionBackground": "#242424",
		"activityBarBadge.background": "#242424",
		"sideBarTitle.foreground": "#DEDEDE",
		"sideBar.background": "#313131",
		"sideBar.foreground": "#DEDEDE",
		"sideBarSectionHeader.background": "#171717",
		"statusBar.background": "#1F1F1F",
		"statusBar.foreground": "#CCCCCC",
		"statusBar.noFolderBackground": "#1F1F1F",
		"dropdown.listBackground": "#313131",
		"dropdown.background": "#313131",
		"dropdown.border": "#242424",
		"editorOverviewRuler.findMatchForeground": "#DEB185",
		"editorOverviewRuler.selectionHighlightForeground": "#ACD474",
		"editorOverviewRuler.errorForeground": "#E08B7E",
		"editorOverviewRuler.warningForeground": "#E08B7E",
		"editorOverviewRuler.infoForeground": "#CCCCCC",
		"editorInfo.foreground": "#CCCCCC",
		"editorHint.foreground": "#BBBBBB",
		"editorWarning.foreground": "#E08B7E",
		"gitDecoration.modifiedResourceForeground": "#E08B7E"
	},
	"tokenColors": [
		{
			"name": "invalid",
			"scope": [
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#F2A0A0",
			}
		},
		{
			"name": "comment",
			"scope": [
				"comment.line",
				"comment.block",
				"comment.block.documentation"
			],
			"settings": {
				"foreground": "#658EC7",
			}
		},
		{
			"name": "comment begin",
			"scope": [
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#4273B6",
			}
		},
		{
			"name": "numeric",
			"scope": [
				"constant.numeric"
			],
			"settings": {
				"foreground": "#E094D5",
			}
		},
		{
			"name": "string",
			"scope": [
				"string"
			],
			"settings": {
				"foreground": "#DEB185",
			}
		},
		{
			"scope": [
				"string.regexp",
				"constant.character.escape"
			],
			"settings": {
				"foreground": "#6ACCB2"
			}
		},
		{
			"name": "boolean, null",
			"scope": [
				"constant.language",
			],
			"settings": {
				"foreground": "#B0AEEB",
			}
		},
		{
			"name": "keyword-import, module",
			"scope": [
				"keyword.control.import",
				"keyword.control.from",
				"keyword.control.export",
				"keyword.control.default",
				"entity.name.import",
				"keyword.package",
				"keyword.import",
				"support.type.object.module",
			],
			"settings": {
				"foreground": "#CCCCCC",
				"fontStyle": "italic"
			}
		},
		{
			"name": "type declaration",
			"scope": [
				"support.type.primitive.ts",
				"constant.other.placeholder",
				"meta.type.annotation",
				"meta.type.parameters.ts",
				"storage.modifier",
			],
			"settings": {
				"foreground": "#A6A7A6",
			}
		},
		{
			"name": "keyword fix",
			"scope": [
				"storage.type.ts",
				"storage.type.function.ts",
				"storage.type.enum.ts",
				"storage.type.function.arrow.ts",
				"storage.type.interface.ts",
				"storage.type.class.ts",
			],
			"settings": {
				"foreground": "#CCCCCC",
			}
		},
		{
			"name": "special keyword",
			"scope": [
				"keyword.generator.asterisk.js",
				"punctuation.definition.string.template.begin",
				"punctuation.definition.string.template.end",
				"punctuation.definition.begin.bracket.curly",
				"punctuation.definition.end.bracket.curly"
			],
			"settings": {
				"foreground": "#7ED3E6",
				"fontStyle": "bold"
			}
		},
		{
			"name": "String interpolation",
			"scope": [
				"punctuation.definition.template-expression.begin",
				"punctuation.definition.template-expression.end",
				"punctuation.section.embedded"
			],
			"settings": {
				"foreground": "#CCCCCC"
			}
		},
		{
			"name": "function, variable, attribute-name, style-property-name",
			"scope": [
				"variable",
				"support.class",
				"meta.object-literal.key",
				"entity.name.type.class.js",
				"entity.name.type.class.ts",
				"entity.name.type.module.ts",
				"support.class.dom.ts",
				"entity.name.type.interface.ts",
				"entity.name.variable",
				"entity.name.function",
				"entity.other.attribute-name",
				"entity.name.function.constructor",
				"entity.name.function.destructor",
				"meta.function-call",
				"support.function",
				"support.variable.property",
				"entity.other.attribute-name.html",
				"support.type.vendored.property-name",
				"variable.css",
				"variable.scss",
				"variable.other.less",
				"source.coffee.embedded"
			],
			"settings": {
				"foreground": "#ACD474",
			}
		},
		{
			"name": "function call",
			"scope": [
				"meta.function-call",
			],
			"settings": {
				"foreground": "#ACD474",
				"fontStyle": "bold"
			}
		},
		{
			"name": "fix css",
			"scope": [
				"variable.parameter.url",
			],
			"settings": {
				"fontStyle": "underline",
				"foreground": "#CCCCCC",
			}
		},
		{
			"name": "debug",
			"scope": [
				"keyword.other.debugger",
				"support.class.console",
				"support.function.console"
			],
			"settings": {
				"foreground": "#E08B7E",
				"fontStyle": "italic"
			}
		},

		{
			"name": "operator",
			"scope": [
				"variable.language.this",
				"keyword.operator",
				"punctuation.accessor",
				"meta.tag.structure.any.html",
				"meta.tag.inline.any.html",
				"keyword.other.unit",
				"punctuation.definition.entity.css"
			],
			"settings": {
				"foreground": "#7ED3E6",
			}
		},
		{
			"name": "punctuation, brackets/braces",
			"scope": [
				"punctuation.definition.block",
				"punctuation.definition.tag",
				"punctuation.definition.typeparameters.begin",
				"punctuation.definition.typeparameters.end"
			],
			"settings": {
				"foreground": "#BBBBBB",
			}
		},
		{
			"name": "module, namespace, package",
			"scope": [
				"meta.namespace",
				"meta.import.js",
				"entity.name.namespace",
			],
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"name": "tag, css selector, css value",
			"scope": [
				"entity.name.tag",
				"meta.selector.css",
				"meta.at-rule.media.header.css",
				"support.constant",
			],
			"settings": {
				"foreground": "#ACD474",
			}
		},
		{
			"name": "inline js keyword, json",
			"scope": [
				"text.html.basic",
				"support.type.property-name",
				"support.constant.property-value.css",
			],
			"settings": {
				"foreground": "#CCCCCC",
			}
		},
		{
			"name": "mardown punctuation",
			"scope": [
				"text.html.markdown punctuation.definition",
				"text.html.markdown beginning.punctuation.definition"
			],
			"settings": {
				"foreground": "#4B4B4B"
			},
		},
		{
			"name": "header",
			"scope": "markup.heading",
			"settings": {
				"foreground": "#ACD474",
				"fontStyle": "bold"
			},
		},
		{
			"name": "italic",
			"scope": "markup.italic",
			"settings": {
				"fontStyle": "italic"
			},
		},
		{
			"name": "underline",
			"scope": "markup.underline",
			"settings": {
				"foreground": "#B0AEEB",
				"fontStyle": "underline"
			},
		},
		{
			"name": "bold",
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold"
			},
		},
		{
			"name": "Lists",
			"scope": "markup.list",
			"settings": {
				"foreground": "#ACD474"
			},
		},
		{
			"name": "List punctuation",
			"scope": "text.html.markdown beginning.punctuation.definition.list",
			"settings": {
				"foreground": "#ACD474"
			},
		},
		{
			"name": "Quotes",
			"scope": "markup.quote",
			"settings": {
				"foreground": "#B0AEEB",
				"fontStyle": "italic"
			},
		},
		{
			"name": "Inserted",
			"scope": "markup.inserted, markup.inserted.git_gutter",
			"settings": {
				"foreground": "#E094D5"
			},
		},
		{
			"name": "Inserted",
			"scope": "markup.inline.raw.string",
			"settings": {
				"foreground": "#E094D5"
			},
		},
		{
			"name": "Deleted",
			"scope": "markup.deleted, markup.deleted.git_gutter",
			"settings": {
				"foreground": "#A6A7A6"
			},
		},
		{
			"name": "Changed",
			"scope": "markup.changed, markup.changed.git_gutter",
			"settings": {
				"foreground": "#E08B7E"
			},
		},
		{
			"name": "Ignored",
			"scope": "markup.ignored, markup.ignored.git_gutter",
			"settings": {
				"foreground": "#A6A7A6"
			},
		},
		{
			"name": "Untracked",
			"scope": "markup.untracked, markup.untracked.git_gutter",
			"settings": {
				"foreground": "#A6A7A6"
			},
		},
		{
			"name": "GitGutter deleted",
			"scope": "markup.deleted.git_gutter",
			"settings": {
				"foreground": "#E08B7E"
			},
		},
		{
			"name": "GitGutter inserted",
			"scope": "markup.inserted.git_gutter",
			"settings": {
				"foreground": "#99cc99"
			},
		},
		{
			"name": "GitGutter changed",
			"scope": "markup.changed.git_gutter",
			"settings": {
				"foreground": "#cc99cc"
			},
		},
		{
			"name": "GitGutter ignored",
			"scope": "markup.ignored.git_gutter",
			"settings": {
				"foreground": "#565656"
			},
		},
		{
			"name": "GitGutter untracked",
			"scope": "markup.untracked.git_gutter",
			"settings": {
				"foreground": "#565656"
			},
		}
	]
}
